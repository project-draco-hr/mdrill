{
  double result=0;
  for (int i=0; i < k; i++) {
    if (m_IsAddition[i]) {
      result+=innerProduct(m_Train.instance(m_Additions[i]),inst);
    }
 else {
      result-=innerProduct(m_Train.instance(m_Additions[i]),inst);
    }
  }
  if (result < 0) {
    return 0;
  }
 else {
    return 1;
  }
}
