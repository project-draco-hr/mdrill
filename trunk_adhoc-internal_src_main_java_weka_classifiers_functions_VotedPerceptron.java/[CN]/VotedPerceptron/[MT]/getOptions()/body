{
  String[] options=new String[8];
  int current=0;
  options[current++]="-I";
  options[current++]="" + m_NumIterations;
  options[current++]="-E";
  options[current++]="" + m_Exponent;
  options[current++]="-S";
  options[current++]="" + m_Seed;
  options[current++]="-M";
  options[current++]="" + m_MaxK;
  while (current < options.length) {
    options[current++]="";
  }
  return options;
}
