{
  if (classAtt.isNumeric()) {
    preds[0]=m_scoreNumeric;
  }
 else {
    if (m_scoreDistributions.size() == 0) {
      preds[m_scoreIndex]=1.0;
    }
 else {
      for (      ScoreDistribution s : m_scoreDistributions) {
        preds[s.getClassLabelIndex()]=s.getConfidence();
      }
    }
  }
}
