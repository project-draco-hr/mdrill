{
  Predicate.Eval currentStatus=Predicate.Eval.FALSE;
  for (  Predicate p : constituents) {
    Predicate.Eval temp=p.evaluate(input);
    if (temp == Predicate.Eval.TRUE) {
      currentStatus=temp;
      break;
    }
 else     if (temp == Predicate.Eval.UNKNOWN) {
      currentStatus=temp;
    }
  }
  return currentStatus;
}
