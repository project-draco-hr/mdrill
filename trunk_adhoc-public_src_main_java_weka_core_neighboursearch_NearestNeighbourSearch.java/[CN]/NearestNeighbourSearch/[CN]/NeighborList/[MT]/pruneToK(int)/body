{
  if (isEmpty()) {
    return;
  }
  if (k < 1) {
    k=1;
  }
  int currentK=0;
  double currentDist=m_First.m_Distance;
  NeighborNode current=m_First;
  for (; current.m_Next != null; current=current.m_Next) {
    currentK++;
    currentDist=current.m_Distance;
    if ((currentK >= k) && (currentDist != current.m_Next.m_Distance)) {
      m_Last=current;
      current.m_Next=null;
      break;
    }
  }
}
