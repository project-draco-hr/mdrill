{
  if (isEmpty()) {
    System.out.println("Empty list");
  }
 else {
    NeighborNode current=m_First;
    while (current != null) {
      System.out.println("Node: instance " + current.m_Instance + ", distance "+ current.m_Distance);
      current=current.m_Next;
    }
    System.out.println();
  }
}
