{
  for (int i=0; i < options.length; i++) {
    if (options[i].equals("--")) {
      options[i++]="";
      String[] result=new String[options.length - i];
      for (int j=i; j < options.length; j++) {
        result[j - i]=options[j];
        options[j]="";
      }
      return result;
    }
  }
  return new String[0];
}
