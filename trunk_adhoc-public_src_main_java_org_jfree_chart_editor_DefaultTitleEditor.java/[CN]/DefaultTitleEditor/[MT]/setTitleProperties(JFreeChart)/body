{
  if (this.showTitle) {
    TextTitle title=chart.getTitle();
    if (title == null) {
      title=new TextTitle();
      chart.setTitle(title);
    }
    title.setText(getTitleText());
    title.setFont(getTitleFont());
    title.setPaint(getTitlePaint());
  }
 else {
    chart.setTitle((TextTitle)null);
  }
}
