{
  if (len > 5 && s[len - 3] == 'e' && s[len - 2] == 's' && s[len - 1] == 't')   return len - 3;
  if (len > 4 && s[len - 2] == 'e' && (s[len - 1] == 'r' || s[len - 1] == 'n'))   return len - 2;
  if (len > 4 && s[len - 2] == 's' && s[len - 1] == 't' && stEnding(s[len - 3]))   return len - 2;
  return len;
}
