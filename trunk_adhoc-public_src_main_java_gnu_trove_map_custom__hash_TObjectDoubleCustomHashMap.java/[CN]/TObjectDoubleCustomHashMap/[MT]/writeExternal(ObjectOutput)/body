{
  out.writeByte(0);
  super.writeExternal(out);
  out.writeObject(strategy);
  out.writeDouble(no_entry_value);
  out.writeInt(_size);
  for (int i=_set.length; i-- > 0; ) {
    if (_set[i] != REMOVED && _set[i] != FREE) {
      out.writeObject(_set[i]);
      out.writeDouble(_values[i]);
    }
  }
}
