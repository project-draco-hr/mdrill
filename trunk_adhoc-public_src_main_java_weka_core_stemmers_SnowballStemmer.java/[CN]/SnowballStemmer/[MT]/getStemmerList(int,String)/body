{
  String result;
  Enumeration enm;
  String name;
  String line;
  result="";
  line="";
  enm=listStemmers();
  while (enm.hasMoreElements()) {
    name=enm.nextElement().toString();
    if (line.length() > 0)     line+=", ";
    if ((lineLength > 0) && (line.length() + name.length() > lineLength)) {
      result+=indention + line + "\n";
      line="";
    }
    line+=name;
  }
  if (line.length() > 0)   result+=indention + line + "\n";
  return result;
}
