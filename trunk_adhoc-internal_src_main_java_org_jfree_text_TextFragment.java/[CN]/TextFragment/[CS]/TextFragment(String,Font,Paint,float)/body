{
  if (text == null) {
    throw new IllegalArgumentException("Null 'text' argument.");
  }
  if (font == null) {
    throw new IllegalArgumentException("Null 'font' argument.");
  }
  if (paint == null) {
    throw new IllegalArgumentException("Null 'paint' argument.");
  }
  this.text=text;
  this.font=font;
  this.paint=paint;
  this.baselineOffset=baselineOffset;
}
