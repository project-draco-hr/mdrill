{
  if (m_children != null) {
    Collection<FPTreeNode> kids=m_children.values();
    for (    FPTreeNode n : kids) {
      text.append("N" + n.m_ID);
      text.append(" [label=\"");
      text.append(n.getItem().toString() + " (" + n.getProjectedCount(0)+ ")\\n");
      text.append("\"]\n");
      n.graphFPTree(text);
      text.append("N" + m_ID + "->"+ "N"+ n.m_ID+ "\n");
    }
  }
}
