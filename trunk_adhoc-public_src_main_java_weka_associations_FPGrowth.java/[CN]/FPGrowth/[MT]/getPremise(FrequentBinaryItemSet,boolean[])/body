{
  boolean ok=false;
  for (int i=0; i < subset.length; i++) {
    if (!subset[i]) {
      ok=true;
      break;
    }
  }
  if (!ok) {
    return null;
  }
  List<Item> premise=new ArrayList<Item>();
  ArrayList<Item> items=new ArrayList<Item>(fis.getItems());
  for (int i=0; i < subset.length; i++) {
    if (subset[i]) {
      premise.add(items.get(i));
    }
  }
  return premise;
}
