{
  String result;
  result=null;
  if (isObject()) {
    if (isRoot())     result="JSON";
 else     if (m_Name == null)     result="<object>";
 else     result=escape(m_Name) + " (Object)";
  }
 else   if (isArray()) {
    if (m_Name == null)     result="<array>";
 else     result=escape(m_Name) + " (Array)";
  }
 else {
    if (m_Name != null)     result=escape(m_Name) + ": " + escape(m_Value);
 else     result="" + m_Value;
  }
  return result;
}
