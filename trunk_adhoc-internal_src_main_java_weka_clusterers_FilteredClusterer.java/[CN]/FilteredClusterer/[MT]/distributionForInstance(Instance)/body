{
  if (m_Filter.numPendingOutput() > 0)   throw new Exception("Filter output queue not empty!");
  if (!m_Filter.input(instance))   throw new Exception("Filter didn't make the test instance immediately available!");
  m_Filter.batchFinished();
  Instance newInstance=m_Filter.output();
  return m_Clusterer.distributionForInstance(newInstance);
}
