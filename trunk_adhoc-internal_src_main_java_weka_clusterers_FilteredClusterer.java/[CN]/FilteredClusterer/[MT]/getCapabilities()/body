{
  Capabilities result;
  if (getFilter() == null) {
    result=super.getCapabilities();
    result.disableAll();
    result.enable(Capability.NO_CLASS);
  }
 else {
    result=getFilter().getCapabilities();
  }
  for (  Capability cap : Capability.values())   result.enableDependency(cap);
  return result;
}
