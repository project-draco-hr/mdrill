{
  PropertyDescriptor result;
  PropertyDescriptor[] desc;
  int i;
  result=null;
  desc=Introspector.getBeanInfo(o.getClass()).getPropertyDescriptors();
  for (i=0; i < desc.length; i++) {
    if (desc[i].getDisplayName().equals(name)) {
      result=desc[i];
      break;
    }
  }
  return result;
}
