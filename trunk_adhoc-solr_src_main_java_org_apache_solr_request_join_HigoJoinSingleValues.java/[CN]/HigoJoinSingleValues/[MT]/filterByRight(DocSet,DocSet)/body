{
  BitDocSet docset=new BitDocSet();
  DocIterator iter=rightDocs.iterator();
  while (iter.hasNext()) {
    int doc=iter.nextDoc();
    IntArr list=join.get(doc);
    if (list == null) {
      continue;
    }
    for (    int jp : list.list) {
      docset.add(jp);
    }
  }
  return leftDocs.intersection(docset);
}
