{
  if (m_plotInstances.numAttributes() > 0 && m_plotInstances.numInstances() > 0) {
    if (j >= 0) {
      m_JitterVal=j;
      m_JRand=new Random(m_JitterVal);
      m_drawnPoints=new int[m_XaxisEnd - m_XaxisStart + 1][m_YaxisEnd - m_YaxisStart + 1];
      updatePturb();
      this.repaint();
    }
  }
}
