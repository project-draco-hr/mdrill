{
  String tmpStr;
  tmpStr=Utils.getOption('S',options);
  if (tmpStr.length() != 0) {
    setRandomSeed(Integer.parseInt(tmpStr));
  }
 else {
    setRandomSeed(1);
  }
  tmpStr=Utils.getOption('Z',options);
  if (tmpStr.length() != 0) {
    setSampleSize(Integer.parseInt(tmpStr));
  }
 else {
    setSampleSize(100);
  }
}
