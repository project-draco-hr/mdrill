{
  if (getWriteMode() == CANCEL) {
    try {
      m_DataBaseConnection.update("DROP TABLE " + m_resolvedTableName);
      if (m_DataBaseConnection.tableExists(m_resolvedTableName))       System.err.println("Table cannot be dropped.");
    }
 catch (    Exception ex) {
      printException(ex);
    }
    resetOptions();
  }
}
