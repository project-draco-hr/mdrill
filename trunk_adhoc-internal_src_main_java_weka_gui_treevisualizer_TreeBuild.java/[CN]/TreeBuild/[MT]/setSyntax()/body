{
  m_st.resetSyntax();
  m_st.eolIsSignificant(false);
  m_st.slashStarComments(true);
  m_st.slashSlashComments(true);
  m_st.whitespaceChars(0,' ');
  m_st.wordChars(' ' + 1,'\u00ff');
  m_st.ordinaryChar('[');
  m_st.ordinaryChar(']');
  m_st.ordinaryChar('{');
  m_st.ordinaryChar('}');
  m_st.ordinaryChar('-');
  m_st.ordinaryChar('>');
  m_st.ordinaryChar('/');
  m_st.ordinaryChar('*');
  m_st.quoteChar('"');
  m_st.whitespaceChars(';',';');
  m_st.ordinaryChar('=');
}
