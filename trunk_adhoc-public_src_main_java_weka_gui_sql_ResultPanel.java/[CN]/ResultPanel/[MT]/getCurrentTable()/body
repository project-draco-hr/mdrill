{
  ResultSetTable table;
  JScrollPane pane;
  JViewport port;
  int index;
  table=null;
  index=m_TabbedPane.getSelectedIndex();
  if (index > -1) {
    pane=(JScrollPane)m_TabbedPane.getComponentAt(index);
    port=(JViewport)pane.getComponent(0);
    table=(ResultSetTable)port.getComponent(0);
  }
  return table;
}
