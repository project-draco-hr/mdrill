{
  boolean result;
  int i;
  int n;
  ExtensionFileFilter[] filters;
  String[] exts;
  result=false;
  filters=getFilters();
  for (i=0; i < filters.length; i++) {
    exts=filters[i].getExtensions();
    for (n=0; n < exts.length; n++) {
      if (file.getName().endsWith(exts[n])) {
        result=true;
        break;
      }
    }
    if (result)     break;
  }
  return result;
}
