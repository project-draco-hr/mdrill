{
  currentDate=v;
  if (isDouble) {
    if (currentDate > lastDate) {
      lastDate=currentDate;
      lastDouble=currentDouble;
    }
 else     if (currentDate < firstDate) {
      firstDate=currentDate;
      firstDouble=currentDouble;
    }
  }
 else {
    if (currentDate > lastDate) {
      lastDate=currentDate;
      lastLong=currentLong;
    }
 else     if (currentDate < firstDate) {
      firstDate=currentDate;
      firstLong=currentLong;
    }
  }
}
