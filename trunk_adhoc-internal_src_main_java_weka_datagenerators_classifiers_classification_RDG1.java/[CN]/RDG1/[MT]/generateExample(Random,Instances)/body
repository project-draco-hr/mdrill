{
  double[] attributes;
  Instance example;
  attributes=new double[getNumAttributes() + 1];
  for (int i=0; i < getNumAttributes(); i++) {
    double value=random.nextDouble();
    if (format.attribute(i).isNumeric()) {
      attributes[i]=value;
    }
 else {
      if (format.attribute(i).isNominal())       attributes[i]=(value > 0.5) ? 1.0 : 0.0;
 else       throw new Exception("Attribute type is not supported.");
    }
  }
  example=new DenseInstance(1.0,attributes);
  example.setDataset(format);
  example.setClassMissing();
  return example;
}
