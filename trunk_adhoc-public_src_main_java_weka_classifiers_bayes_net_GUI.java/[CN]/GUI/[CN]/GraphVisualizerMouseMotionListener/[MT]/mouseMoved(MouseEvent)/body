{
  int iNode=getGraphNode(me);
  if (iNode >= 0) {
    if (iNode != m_nLastNode) {
      m_GraphPanel.highLight(iNode);
      if (m_nLastNode >= 0) {
        m_GraphPanel.highLight(m_nLastNode);
      }
      m_nLastNode=iNode;
    }
  }
  if (iNode < 0 && m_nLastNode >= 0) {
    m_GraphPanel.repaint();
    m_nLastNode=-1;
  }
}
