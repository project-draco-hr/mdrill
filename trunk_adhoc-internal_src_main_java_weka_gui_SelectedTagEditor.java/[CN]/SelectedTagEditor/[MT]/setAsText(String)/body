{
  SelectedTag s=(SelectedTag)getValue();
  Tag[] tags=s.getTags();
  try {
    for (int i=0; i < tags.length; i++) {
      if (text.equals(tags[i].getReadable())) {
        setValue(new SelectedTag(tags[i].getID(),tags));
        return;
      }
    }
  }
 catch (  Exception ex) {
    throw new java.lang.IllegalArgumentException(text);
  }
}
