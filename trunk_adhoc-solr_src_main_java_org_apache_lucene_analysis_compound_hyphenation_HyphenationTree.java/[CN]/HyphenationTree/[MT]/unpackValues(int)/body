{
  StringBuilder buf=new StringBuilder();
  byte v=vspace.get(k++);
  while (v != 0) {
    char c=(char)((v >>> 4) - 1 + '0');
    buf.append(c);
    c=(char)(v & 0x0f);
    if (c == 0) {
      break;
    }
    c=(char)(c - 1 + '0');
    buf.append(c);
    v=vspace.get(k++);
  }
  return buf.toString();
}
