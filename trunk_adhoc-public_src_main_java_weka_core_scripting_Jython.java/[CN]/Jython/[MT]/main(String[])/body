{
  if (args.length == 0) {
    System.out.println("Jython present: " + isPresent());
  }
 else {
    Jython jython=new Jython();
    if (jython.getInterpreter() == null)     System.err.println("Cannot instantiate Python Interpreter!");
 else     jython.invoke("execfile",new Class[]{String.class},new Object[]{args[0]});
  }
}
