{
  boolean result;
  Class cls;
  result=true;
  cls=null;
  try {
    cls=Class.forName(getClassname());
  }
 catch (  Exception e) {
    result=false;
    println("Cannot instantiate '" + getClassname() + "'! Class in CLASSPATH?");
  }
  if (result) {
    try {
      cls.newInstance();
    }
 catch (    Exception e) {
      result=false;
      println("Cannot instantiate '" + getClassname() + "'! Missing default constructor?");
    }
  }
  return result;
}
