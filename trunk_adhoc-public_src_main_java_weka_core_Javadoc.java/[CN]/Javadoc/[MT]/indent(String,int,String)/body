{
  String result;
  StringTokenizer tok;
  int i;
  tok=new StringTokenizer(content,"\n",true);
  result="";
  while (tok.hasMoreTokens()) {
    if (result.endsWith("\n") || (result.length() == 0)) {
      for (i=0; i < count; i++)       result+=indentStr;
    }
    result+=tok.nextToken();
  }
  return result;
}
