{
  double numerator;
  double noUnknown;
  double unknownRate;
  int i;
  noUnknown=totalNoInst - bags.total();
  unknownRate=noUnknown / totalNoInst;
  numerator=(oldEnt(bags) - newEnt(bags));
  numerator=(1 - unknownRate) * numerator;
  if (Utils.eq(numerator,0))   return 0;
  return numerator / bags.total();
}
