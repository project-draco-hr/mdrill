{
  AbstractOutput result;
  String[] options;
  String classname;
  try {
    options=Utils.splitOptions(cmdline);
    classname=options[0];
    options[0]="";
    result=(AbstractOutput)Utils.forName(AbstractOutput.class,classname,options);
  }
 catch (  Exception e) {
    result=null;
  }
  return result;
}
