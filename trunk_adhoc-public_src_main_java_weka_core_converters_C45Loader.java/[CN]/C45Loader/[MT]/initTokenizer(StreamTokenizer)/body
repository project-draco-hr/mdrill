{
  tokenizer.resetSyntax();
  tokenizer.whitespaceChars(0,(' ' - 1));
  tokenizer.wordChars(' ','\u00FF');
  tokenizer.whitespaceChars(',',',');
  tokenizer.whitespaceChars(':',':');
  tokenizer.commentChar('|');
  tokenizer.whitespaceChars('\t','\t');
  tokenizer.quoteChar('"');
  tokenizer.quoteChar('\'');
  tokenizer.eolIsSignificant(true);
}
