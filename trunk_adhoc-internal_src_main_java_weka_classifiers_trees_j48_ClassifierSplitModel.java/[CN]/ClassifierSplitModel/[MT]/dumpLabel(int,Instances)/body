{
  StringBuffer text;
  text=new StringBuffer();
  text.append(((Instances)data).classAttribute().value(m_distribution.maxClass(index)));
  text.append(" (" + Utils.roundDouble(m_distribution.perBag(index),2));
  if (Utils.gr(m_distribution.numIncorrect(index),0))   text.append("/" + Utils.roundDouble(m_distribution.numIncorrect(index),2));
  text.append(")");
  return text.toString();
}
