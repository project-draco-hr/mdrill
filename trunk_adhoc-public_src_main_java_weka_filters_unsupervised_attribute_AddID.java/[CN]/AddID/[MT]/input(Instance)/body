{
  if (getInputFormat() == null)   throw new IllegalStateException("No input instance format defined");
  if (m_NewBatch) {
    resetQueue();
    m_NewBatch=false;
  }
  if (!isFirstBatchDone()) {
    bufferInput(instance);
    return false;
  }
 else {
    convertInstance(instance);
    return true;
  }
}
