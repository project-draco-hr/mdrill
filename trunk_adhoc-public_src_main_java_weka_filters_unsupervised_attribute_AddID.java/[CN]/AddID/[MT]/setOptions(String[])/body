{
  String tmpStr;
  tmpStr=Utils.getOption('C',options);
  if (tmpStr.length() != 0)   m_Index.setSingleIndex(tmpStr);
 else   m_Index.setSingleIndex("first");
  tmpStr=Utils.getOption('N',options);
  if (tmpStr.length() != 0)   m_Name=tmpStr;
 else   m_Name="ID";
  if (getInputFormat() != null)   setInputFormat(getInputFormat());
}
