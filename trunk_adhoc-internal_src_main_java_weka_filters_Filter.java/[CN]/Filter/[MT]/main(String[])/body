{
  try {
    if (args.length == 0) {
      throw new Exception("First argument must be the class name of a Filter");
    }
    String fname=args[0];
    Filter f=(Filter)Class.forName(fname).newInstance();
    args[0]="";
    runFilter(f,args);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    System.err.println(ex.getMessage());
  }
}
