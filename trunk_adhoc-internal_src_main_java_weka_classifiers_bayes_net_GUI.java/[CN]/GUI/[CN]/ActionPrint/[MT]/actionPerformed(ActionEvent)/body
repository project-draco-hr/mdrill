{
  PrinterJob printJob=PrinterJob.getPrinterJob();
  printJob.setPrintable(m_GraphPanel);
  if (printJob.printDialog())   try {
    m_bIsPrinting=true;
    printJob.print();
    m_bIsPrinting=false;
  }
 catch (  PrinterException pe) {
    m_jStatusBar.setText("Error printing: " + pe);
    m_bIsPrinting=false;
  }
  m_jStatusBar.setText("Print");
}
