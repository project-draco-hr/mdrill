{
  String seedString=Utils.getOption('R',options);
  if (seedString.length() != 0)   m_Seed=Long.parseLong(seedString);
 else   m_Seed=1;
  String orderString=Utils.getOption('C',options);
  if (orderString.length() != 0)   m_ClassOrder=Integer.parseInt(orderString);
 else   m_ClassOrder=FREQ_ASCEND;
  if (getInputFormat() != null)   setInputFormat(getInputFormat());
  m_Random=null;
}
