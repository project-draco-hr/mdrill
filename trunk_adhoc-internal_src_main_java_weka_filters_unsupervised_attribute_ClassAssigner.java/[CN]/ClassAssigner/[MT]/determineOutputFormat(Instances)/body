{
  Instances result=new Instances(inputFormat,0);
  if (m_ClassIndex == FIRST)   result.setClassIndex(0);
 else   if (m_ClassIndex == LAST)   result.setClassIndex(result.numAttributes() - 1);
 else   if (m_ClassIndex == UNSET)   result.setClassIndex(-1);
 else   result.setClassIndex(m_ClassIndex);
  return result;
}
