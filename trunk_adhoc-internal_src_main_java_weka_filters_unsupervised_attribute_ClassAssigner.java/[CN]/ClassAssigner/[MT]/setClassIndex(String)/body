{
  if (value.equalsIgnoreCase("first")) {
    m_ClassIndex=FIRST;
  }
 else   if (value.equalsIgnoreCase("last")) {
    m_ClassIndex=LAST;
  }
 else   if (value.equalsIgnoreCase("0")) {
    m_ClassIndex=UNSET;
  }
 else {
    try {
      m_ClassIndex=Integer.parseInt(value) - 1;
    }
 catch (    Exception e) {
      System.err.println("Error parsing '" + value + "'!");
    }
  }
}
