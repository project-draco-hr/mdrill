{
  double maxF=-Double.MAX_VALUE;
  for (int i=0; i < Fs.length; i++) {
    if (Fs[i] > maxF) {
      maxF=Fs[i];
    }
  }
  double sum=0;
  double[] probs=new double[Fs.length];
  for (int i=0; i < Fs.length; i++) {
    probs[i]=Math.exp(Fs[i] - maxF);
    sum+=probs[i];
  }
  Utils.normalize(probs,sum);
  return probs;
}
