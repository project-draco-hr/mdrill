{
  double maxCount=0;
  int maxIndex=0;
  int i;
  if (Utils.gr(m_perBag[index],0)) {
    for (i=0; i < m_perClass.length; i++)     if (Utils.gr(m_perClassPerBag[index][i],maxCount)) {
      maxCount=m_perClassPerBag[index][i];
      maxIndex=i;
    }
    return maxIndex;
  }
 else   return maxClass();
}
