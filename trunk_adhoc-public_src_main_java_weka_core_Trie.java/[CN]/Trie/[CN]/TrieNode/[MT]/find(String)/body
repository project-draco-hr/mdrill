{
  TrieNode result;
  Character c;
  String newSuffix;
  TrieNode child;
  c=suffix.charAt(0);
  newSuffix=suffix.substring(1);
  child=m_Children.get(c);
  if (child == null)   result=null;
 else   if (newSuffix.length() == 0)   result=child;
 else   result=child.find(newSuffix);
  return result;
}
