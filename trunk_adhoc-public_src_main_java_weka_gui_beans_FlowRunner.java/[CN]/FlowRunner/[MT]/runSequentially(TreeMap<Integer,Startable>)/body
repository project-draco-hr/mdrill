{
  Set<Integer> s=startables.keySet();
  for (  Integer i : s) {
    try {
      Startable startPoint=startables.get(i);
      startPoint.start();
      Thread.sleep(200);
      waitUntilFinished();
    }
 catch (    Exception ex) {
      ex.printStackTrace();
      if (m_log != null) {
        m_log.logMessage(ex.getMessage());
        m_log.logMessage("Aborting...");
      }
 else {
        System.err.println(ex.getMessage());
        System.err.println("Aborting...");
      }
      break;
    }
  }
}
