{
  File resultsFile;
  try {
    resultsFile=File.createTempFile("weka_experiment",".arff");
    resultsFile.deleteOnExit();
  }
 catch (  Exception e) {
    System.err.println("Cannot create temp file, writing to standard out.");
    resultsFile=new File("-");
  }
  setOutputFile(resultsFile);
  setOutputFileName("");
}
