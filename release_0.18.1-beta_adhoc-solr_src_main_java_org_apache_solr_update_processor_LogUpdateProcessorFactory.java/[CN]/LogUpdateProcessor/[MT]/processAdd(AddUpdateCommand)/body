{
  if (adds == null) {
    adds=new ArrayList<String>();
    toLog.add("add",adds);
  }
  if (adds.size() < maxNumToLog) {
    adds.add(cmd.getPrintableId(req.getSchema()));
  }
  if (logDebug) {
    log.debug("add {}",cmd.getPrintableId(req.getSchema()));
  }
  numAdds++;
  if (next != null)   next.processAdd(cmd);
}
