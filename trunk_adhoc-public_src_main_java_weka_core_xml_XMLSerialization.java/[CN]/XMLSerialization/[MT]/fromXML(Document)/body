{
  if (!document.getDocumentElement().getNodeName().equals(ROOT_NODE))   throw new Exception("Expected '" + ROOT_NODE + "' as root element, but found '"+ document.getDocumentElement().getNodeName()+ "'!");
  m_Document.setDocument(readPreProcess(document));
  checkVersion();
  return readPostProcess(invokeReadFromXML(m_Document.getDocument().getDocumentElement()));
}
