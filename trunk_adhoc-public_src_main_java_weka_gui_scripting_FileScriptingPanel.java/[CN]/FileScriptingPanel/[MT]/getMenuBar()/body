{
  JMenuBar result;
  JMenu menu;
  JMenuItem menuitem;
  result=new JMenuBar();
  menu=new JMenu("File");
  menu.setMnemonic('F');
  result.add(menu);
  menuitem=new JMenuItem(m_NewAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_OpenAction);
  menu.addSeparator();
  menu.add(menuitem);
  menuitem=new JMenuItem(m_SaveAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_SaveAsAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_PrintAction);
  menu.addSeparator();
  menu.add(menuitem);
  menuitem=new JMenuItem(m_ClearOutputAction);
  menu.addSeparator();
  menu.add(menuitem);
  menuitem=new JMenuItem(m_ExitAction);
  menu.addSeparator();
  menu.add(menuitem);
  menu=new JMenu("Edit");
  menu.setMnemonic('E');
  result.add(menu);
  menuitem=new JMenuItem(m_UndoAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_RedoAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_CutAction);
  menu.addSeparator();
  menu.add(menuitem);
  menuitem=new JMenuItem(m_CopyAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_PasteAction);
  menu.add(menuitem);
  menu=new JMenu("Script");
  menu.setMnemonic('S');
  result.add(menu);
  menuitem=new JMenuItem(m_StartAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_StopAction);
  menu.add(menuitem);
  menuitem=new JMenuItem(m_ArgsAction);
  menu.add(menuitem);
  menu=new JMenu("Help");
  menu.setMnemonic('H');
  result.add(menu);
  menuitem=new JMenuItem(m_AboutAction);
  menu.add(menuitem);
  return result;
}
