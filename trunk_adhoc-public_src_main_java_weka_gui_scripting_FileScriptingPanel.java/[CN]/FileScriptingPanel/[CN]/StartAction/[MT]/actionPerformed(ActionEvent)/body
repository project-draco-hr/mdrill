{
  if (!checkModified())   return;
  if (m_Script.getFilename() == null)   return;
  try {
    m_Script.start(m_Args);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    JOptionPane.showMessageDialog(FileScriptingPanel.this,"Error running script:\n" + ex,"Error",JOptionPane.ERROR_MESSAGE);
  }
}
