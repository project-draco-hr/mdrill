{
  long pos=0;
  try {
    String termText=termsCache.get(i);
    if (termText != null) {
      return termText;
    }
    pos=indexl[i];
    quicktisInput.quicktisInputTxt.seek(pos);
    termText=quicktisInput.quicktisInputTxt.readString();
    termsCache.put(i,termText);
    return termText;
  }
 catch (  Throwable e) {
    log.error(i + "@pos:" + pos,e);
    return "null";
  }
}
