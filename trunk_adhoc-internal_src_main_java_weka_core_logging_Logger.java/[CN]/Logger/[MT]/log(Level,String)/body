{
  Logger logger;
  boolean log;
  String[] location;
  logger=getSingleton();
  if (logger == null)   return;
synchronized (logger) {
    log=false;
    if (logger.getMinLevel() == Level.ALL)     log=true;
 else     if (level.getOrder() >= logger.getMinLevel().getOrder())     log=true;
    if (!log)     return;
    location=getLocation();
    logger.doLog(level,msg,location[0],location[1],Integer.parseInt(location[2]));
  }
}
