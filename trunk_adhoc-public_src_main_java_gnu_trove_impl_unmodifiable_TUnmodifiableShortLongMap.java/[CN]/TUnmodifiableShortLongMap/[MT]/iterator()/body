{
  return new TShortLongIterator(){
    TShortLongIterator iter=m.iterator();
    public short key(){
      return iter.key();
    }
    public long value(){
      return iter.value();
    }
    public void advance(){
      iter.advance();
    }
    public boolean hasNext(){
      return iter.hasNext();
    }
    public long setValue(    long val){
      throw new UnsupportedOperationException();
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
