{
  if (jj_3R_116())   return true;
  if (jj_scan_token(K_UNION))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_121())   jj_scanpos=xsp;
  if (jj_3R_116())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_122()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
