{
  if (!m_indicators[index]) {
    if (m_number == 0) {
      m_first=index;
      m_next[index]=-1;
      m_previous[index]=-1;
    }
 else {
      m_previous[m_first]=index;
      m_next[index]=m_first;
      m_previous[index]=-1;
      m_first=index;
    }
    m_indicators[index]=true;
    m_number++;
  }
}
