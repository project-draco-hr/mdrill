{
  if (inputFormatPeek() == null) {
    throw new NullPointerException("No input instance format defined");
  }
  if (m_NewBatch) {
    resetQueue();
    m_NewBatch=false;
  }
  if (m_firstBatchFinished) {
    push(instance);
    return true;
  }
 else {
    bufferInput(instance);
    return false;
  }
}
