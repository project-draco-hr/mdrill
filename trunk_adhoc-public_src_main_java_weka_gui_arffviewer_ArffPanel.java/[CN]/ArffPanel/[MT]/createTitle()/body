{
  File file;
  if (m_Filename.equals("")) {
    m_Title="-none-";
  }
 else   if (m_Filename.equals(TAB_INSTANCES)) {
    m_Title=TAB_INSTANCES;
  }
 else {
    try {
      file=new File(m_Filename);
      m_Title=file.getName();
    }
 catch (    Exception e) {
      m_Title="-none-";
    }
  }
  if (isChanged())   m_Title+=" *";
}
