{
  boolean result=false;
switch (m_closure) {
case OPENCLOSED:
    if (value > m_leftMargin && value <= m_rightMargin) {
      result=true;
    }
  break;
case OPENOPEN:
if (value > m_leftMargin && value < m_rightMargin) {
  result=true;
}
break;
case CLOSEDOPEN:
if (value >= m_leftMargin && value < m_rightMargin) {
result=true;
}
break;
case CLOSEDCLOSED:
if (value >= m_leftMargin && value <= m_rightMargin) {
result=true;
}
break;
default :
result=false;
break;
}
return result;
}
