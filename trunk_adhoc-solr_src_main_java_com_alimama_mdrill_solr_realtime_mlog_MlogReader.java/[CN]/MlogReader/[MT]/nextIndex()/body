{
  if (this.currReader != null) {
    this.currReader.close();
    this.currReader=null;
  }
  this.currIndex++;
  if (this.currIndex >= this.list.size()) {
    return false;
  }
  this.setCurrIndex(this.currIndex);
  this.currReader=new MLogSigmentReader(conf,filepath,currStart);
  return true;
}
