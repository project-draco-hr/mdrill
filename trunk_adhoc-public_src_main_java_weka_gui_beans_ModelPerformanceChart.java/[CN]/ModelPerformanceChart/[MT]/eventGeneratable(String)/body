{
  if (m_listenees.size() == 0) {
    return false;
  }
  boolean ok=false;
  for (  Object o : m_listenees) {
    if (o instanceof EventConstraints) {
      if (((EventConstraints)o).eventGeneratable("thresholdData") || ((EventConstraints)o).eventGeneratable("visualizableError")) {
        ok=true;
        break;
      }
    }
  }
  return ok;
}
