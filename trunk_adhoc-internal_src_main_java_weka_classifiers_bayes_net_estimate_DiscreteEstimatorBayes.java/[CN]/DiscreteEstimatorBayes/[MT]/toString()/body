{
  String result="Discrete Estimator. Counts = ";
  if (m_SumOfCounts > 1) {
    for (int i=0; i < m_Counts.length; i++) {
      result+=" " + Utils.doubleToString(m_Counts[i],2);
    }
    result+="  (Total = " + Utils.doubleToString(m_SumOfCounts,2) + ")\n";
  }
 else {
    for (int i=0; i < m_Counts.length; i++) {
      result+=" " + m_Counts[i];
    }
    result+="  (Total = " + m_SumOfCounts + ")\n";
  }
  return result;
}
