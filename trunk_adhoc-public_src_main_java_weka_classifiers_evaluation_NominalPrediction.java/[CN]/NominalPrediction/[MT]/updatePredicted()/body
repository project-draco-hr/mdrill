{
  int predictedClass=-1;
  double bestProb=0.0;
  for (int i=0; i < m_Distribution.length; i++) {
    if (m_Distribution[i] > bestProb) {
      predictedClass=i;
      bestProb=m_Distribution[i];
    }
  }
  if (predictedClass != -1) {
    m_Predicted=predictedClass;
  }
 else {
    m_Predicted=MISSING_VALUE;
  }
}
