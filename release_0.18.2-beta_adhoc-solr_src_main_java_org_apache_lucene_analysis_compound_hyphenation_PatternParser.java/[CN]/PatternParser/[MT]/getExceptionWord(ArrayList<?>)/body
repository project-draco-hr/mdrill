{
  StringBuilder res=new StringBuilder();
  for (int i=0; i < ex.size(); i++) {
    Object item=ex.get(i);
    if (item instanceof String) {
      res.append((String)item);
    }
 else {
      if (((Hyphen)item).noBreak != null) {
        res.append(((Hyphen)item).noBreak);
      }
    }
  }
  return res.toString();
}
