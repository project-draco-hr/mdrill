{
  double newVal=value;
  if (m_outlierTreatmentMethod != Outlier.ASIS) {
    if (m_outlierTreatmentMethod == Outlier.ASMISSINGVALUES) {
      newVal=applyMissingValueTreatment(value);
    }
 else {
      if (value < m_lowValue) {
        newVal=m_lowValue;
      }
 else       if (value > m_highValue) {
        newVal=m_highValue;
      }
    }
  }
  return newVal;
}
