{
  if (getInputFormat() == null) {
    throw new NullPointerException("No input instance format defined");
  }
  if (m_NewBatch) {
    resetQueue();
    m_NewBatch=false;
    resetHistory();
  }
  Instance newInstance=historyInput(instance);
  if (newInstance != null) {
    push(newInstance);
    return true;
  }
 else {
    return false;
  }
}
