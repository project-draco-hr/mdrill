{
  StringBuffer result=new StringBuffer();
  result.append(m_attribute.name() + " ");
switch (m_comparison) {
case EQUAL:
    result.append("=");
  break;
case LESS_THAN_OR_EQUAL_TO:
result.append("<=");
break;
case GREATER_THAN:
result.append(">");
break;
}
result.append(" " + Utils.doubleToString(m_splitPoint,4));
if (freq) {
result.append(":" + m_frequency);
}
return result.toString();
}
