{
  if (!isConnected())   throw new IllegalStateException("Not connected, please connect first!");
  if (!isCursorScrollable())   m_PreparedStatement=m_Connection.prepareStatement(query,ResultSet.TYPE_FORWARD_ONLY,ResultSet.CONCUR_READ_ONLY);
 else   m_PreparedStatement=m_Connection.prepareStatement(query,getSupportedCursorScrollType(),ResultSet.CONCUR_READ_ONLY);
  return (m_PreparedStatement.execute());
}
