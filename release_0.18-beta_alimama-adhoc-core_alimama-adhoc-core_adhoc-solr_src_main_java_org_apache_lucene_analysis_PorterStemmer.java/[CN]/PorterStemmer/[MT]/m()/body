{
  int n=0;
  int i=k0;
  while (true) {
    if (i > j)     return n;
    if (!cons(i))     break;
    i++;
  }
  i++;
  while (true) {
    while (true) {
      if (i > j)       return n;
      if (cons(i))       break;
      i++;
    }
    i++;
    n++;
    while (true) {
      if (i > j)       return n;
      if (!cons(i))       break;
      i++;
    }
    i++;
  }
}
