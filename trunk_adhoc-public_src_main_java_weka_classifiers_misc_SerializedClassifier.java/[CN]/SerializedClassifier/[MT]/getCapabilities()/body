{
  Capabilities result;
  if (m_ModelFile != null && m_ModelFile.exists() && m_ModelFile.isFile()) {
    try {
      initModel();
    }
 catch (    Exception e) {
      System.err.println(e);
    }
  }
  if (m_Model != null) {
    result=m_Model.getCapabilities();
  }
 else {
    result=new Capabilities(this);
    result.disableAll();
  }
  for (  Capability cap : Capability.values())   result.enableDependency(cap);
  result.setOwner(this);
  return result;
}
