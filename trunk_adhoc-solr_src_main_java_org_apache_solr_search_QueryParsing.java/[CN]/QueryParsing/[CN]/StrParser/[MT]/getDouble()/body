{
  eatws();
  char[] arr=new char[end - pos];
  int i;
  for (i=0; i < arr.length; i++) {
    char ch=val.charAt(pos);
    if ((ch >= '0' && ch <= '9') || ch == '+' || ch == '-' || ch == '.' || ch == 'e' || ch == 'E') {
      pos++;
      arr[i]=ch;
    }
 else {
      break;
    }
  }
  return Double.parseDouble(new String(arr,0,i));
}
