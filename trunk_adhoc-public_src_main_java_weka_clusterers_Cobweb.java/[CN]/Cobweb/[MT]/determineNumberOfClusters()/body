{
  if (!m_numberOfClustersDetermined && (m_cobwebTree != null)) {
    int[] numClusts=new int[1];
    numClusts[0]=0;
    try {
      m_cobwebTree.assignClusterNums(numClusts);
    }
 catch (    Exception e) {
      e.printStackTrace();
      numClusts[0]=0;
    }
    m_numberOfClusters=numClusts[0];
    m_numberOfClustersDetermined=true;
  }
}
