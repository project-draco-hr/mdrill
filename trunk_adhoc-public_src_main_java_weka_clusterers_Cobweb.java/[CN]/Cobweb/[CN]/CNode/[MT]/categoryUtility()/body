{
  if (m_children == null) {
    throw new Exception("categoryUtility: No children!");
  }
  double totalCU=0;
  for (int i=0; i < m_children.size(); i++) {
    CNode child=(CNode)m_children.elementAt(i);
    totalCU+=categoryUtilityChild(child);
  }
  totalCU/=(double)m_children.size();
  return totalCU;
}
