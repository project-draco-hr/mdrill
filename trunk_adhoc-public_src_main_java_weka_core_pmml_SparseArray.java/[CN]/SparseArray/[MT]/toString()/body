{
  StringBuffer text=new StringBuffer();
  text.append("<");
  for (int i=0; i < m_indices.size(); i++) {
    text.append(m_indices.get(i).toString() + " ");
    text.append(m_values.get(i));
    if (i < m_indices.size() - 1) {
      text.append(",");
    }
  }
  text.append(">");
  return text.toString();
}
