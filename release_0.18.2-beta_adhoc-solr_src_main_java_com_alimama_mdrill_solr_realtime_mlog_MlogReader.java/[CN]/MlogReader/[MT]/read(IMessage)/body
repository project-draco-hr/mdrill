{
  while (true) {
    try {
      if (minStart >= maxEnd) {
        return false;
      }
      if (this.currReader == null) {
        this.currReader=new MLogSigmentReader(conf,filepath,currStart);
        this.currReader=null;
      }
      if (this.currReader.getPos() >= this.currEnd) {
        if (!this.nextIndex()) {
          return false;
        }
      }
      this.currReader.read(msg);
      return true;
    }
 catch (    IOException e) {
      try {
        this.nextIndex();
      }
 catch (      IOException e1) {
      }
    }
  }
}
