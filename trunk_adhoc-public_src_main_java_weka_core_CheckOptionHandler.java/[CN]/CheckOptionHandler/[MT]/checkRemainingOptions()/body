{
  boolean result;
  String[] options;
  print("Remaining options...");
  options=getUserOptions();
  try {
    getDefaultHandler().setOptions(options);
    if (getDebug())     println("\n  remaining: " + printOptions(options));
    println("yes");
    result=true;
  }
 catch (  Exception e) {
    println("no");
    result=false;
    if (getDebug())     println(e);
  }
  return result;
}
