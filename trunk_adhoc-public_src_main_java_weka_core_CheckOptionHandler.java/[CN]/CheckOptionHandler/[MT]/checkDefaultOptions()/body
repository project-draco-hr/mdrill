{
  boolean result;
  String[] options;
  print("Default options...");
  options=getDefaultOptions();
  try {
    getDefaultHandler().setOptions(options);
    Utils.checkForRemainingOptions(options);
    println("yes");
    result=true;
  }
 catch (  Exception e) {
    println("no");
    result=false;
    if (getDebug())     println(e);
  }
  return result;
}
