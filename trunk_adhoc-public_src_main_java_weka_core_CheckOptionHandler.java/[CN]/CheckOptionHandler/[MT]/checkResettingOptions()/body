{
  boolean result;
  String[] defaultOptions;
  String[] defaultOptionsCheck;
  OptionHandler handler;
  print("Resetting options...");
  try {
    if (getDebug())     println("\n  Setting user options");
    handler=getDefaultHandler();
    handler.setOptions(getUserOptions());
    defaultOptions=getDefaultOptions();
    if (getDebug())     println("  Resetting to default options");
    handler.setOptions(getCopy(defaultOptions));
    if (getDebug())     println("  Checking default options match previous default");
    defaultOptionsCheck=handler.getOptions();
    compareOptions(defaultOptions,defaultOptionsCheck);
    println("yes");
    result=true;
  }
 catch (  Exception e) {
    println("no");
    result=false;
    if (getDebug())     println(e);
  }
  return result;
}
