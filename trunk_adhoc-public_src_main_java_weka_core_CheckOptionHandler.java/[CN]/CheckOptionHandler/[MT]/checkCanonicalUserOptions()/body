{
  boolean result;
  OptionHandler handler;
  String[] userOptions;
  String[] userOptionsCheck;
  print("Canonical user options...");
  try {
    handler=getDefaultHandler();
    handler.setOptions(getUserOptions());
    if (getDebug())     print("\n  Getting canonical user options: ");
    userOptions=handler.getOptions();
    if (getDebug())     println(printOptions(userOptions));
    if (getDebug())     println("  Setting canonical user options");
    handler.setOptions((String[])userOptions.clone());
    if (getDebug())     println("  Checking canonical user options");
    userOptionsCheck=handler.getOptions();
    compareOptions(userOptions,userOptionsCheck);
    println("yes");
    result=true;
  }
 catch (  Exception e) {
    println("no");
    result=false;
    if (getDebug())     println(e);
  }
  return result;
}
