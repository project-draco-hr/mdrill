{
  int i;
  int n;
  MetaBean meta;
  Vector subflow;
  BeanInstance beaninst;
  for (i=0; i < metabeans.size(); i++) {
    meta=(MetaBean)metabeans.get(i);
    subflow=meta.getSubFlow();
    for (n=0; n < subflow.size(); n++) {
      beaninst=(BeanInstance)subflow.get(n);
      beaninst.removeBean(m_BeanLayout);
    }
  }
}
