{
  if (memsize >= 0) {
    return memsize;
  }
  memsize=64;
  for (  Entry<Integer,IntArr> e : join.entrySet()) {
    memsize+=16;
    memsize+=e.getValue().memsize();
  }
  for (  Entry<Integer,IntArr> e : joinRevert.entrySet()) {
    memsize+=16;
    memsize+=e.getValue().memsize();
  }
  return memsize;
}
