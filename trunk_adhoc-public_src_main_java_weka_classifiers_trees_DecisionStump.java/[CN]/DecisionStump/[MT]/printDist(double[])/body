{
  StringBuffer text=new StringBuffer();
  if (m_Instances.classAttribute().isNominal()) {
    for (int i=0; i < m_Instances.numClasses(); i++) {
      text.append(m_Instances.classAttribute().value(i) + "\t");
    }
    text.append("\n");
    for (int i=0; i < m_Instances.numClasses(); i++) {
      text.append(dist[i] + "\t");
    }
    text.append("\n");
  }
  return text.toString();
}
