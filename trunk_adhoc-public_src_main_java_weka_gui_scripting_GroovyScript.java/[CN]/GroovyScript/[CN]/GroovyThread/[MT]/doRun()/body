{
  Object groovy;
  groovy=Groovy.newInstance(m_Owner.getFilename(),Object.class);
  if (hasMethod(groovy,"run"))   Groovy.invoke(groovy,"run",new Class[]{String[].class},new Object[]{getArgs()});
 else   if (hasMethod(groovy,"main"))   Groovy.invoke(groovy,"main",new Class[]{String[].class},new Object[]{getArgs()});
 else   throw new IllegalStateException("Neither 'run' nor 'main' method found!");
}
