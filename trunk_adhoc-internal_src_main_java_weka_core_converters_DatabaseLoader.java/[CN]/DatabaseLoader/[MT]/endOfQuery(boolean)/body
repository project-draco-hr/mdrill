{
  String table;
  int beginIndex, endIndex;
  beginIndex=m_query.indexOf("FROM ") + 5;
  while (m_query.charAt(beginIndex) == ' ')   beginIndex++;
  endIndex=m_query.indexOf(" ",beginIndex);
  if (endIndex != -1 && onlyTableName)   table=m_query.substring(beginIndex,endIndex);
 else   table=m_query.substring(beginIndex);
  if (m_DataBaseConnection.getUpperCase())   table=table.toUpperCase();
  return table;
}
