{
  NamedList rtn=new NamedList();
  rtn.add("count",value);
  if (isFinalResult) {
    NamedList stat=new NamedList();
    stat.add("sum",0d);
    stat.add("max",0d);
    stat.add("min",0d);
    stat.add("dist",0d);
    rtn.add("higo_empty_s",stat);
  }
 else {
    rtn.add("rc",2);
    rtn.add("docid",this.docid);
    if (colVal != null) {
      rtn.add("sortStr",colVal);
    }
  }
  return rtn;
}
