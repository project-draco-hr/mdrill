{
  if (!fileName.endsWith(".kf") && !fileName.endsWith(".kfml")) {
    throw new Exception("Can only load and run binary or xml serialized KnowledgeFlows " + "(*.kf | *.kfml)");
  }
  if (fileName.endsWith(".kf")) {
    loadBinary(fileName);
  }
 else   if (fileName.endsWith(".kfml")) {
    loadXML(fileName);
  }
}
