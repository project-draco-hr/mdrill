{
  if (instance.isMissing(m_AttIndex)) {
    return 2;
  }
 else   if (instance.attribute(m_AttIndex).isNominal()) {
    if ((int)instance.value(m_AttIndex) == m_SplitPoint) {
      return 0;
    }
 else {
      return 1;
    }
  }
 else {
    if (instance.value(m_AttIndex) <= m_SplitPoint) {
      return 0;
    }
 else {
      return 1;
    }
  }
}
