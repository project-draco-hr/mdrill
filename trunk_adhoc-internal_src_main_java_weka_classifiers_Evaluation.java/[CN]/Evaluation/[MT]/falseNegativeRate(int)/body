{
  double incorrect=0, total=0;
  for (int i=0; i < m_NumClasses; i++) {
    if (i == classIndex) {
      for (int j=0; j < m_NumClasses; j++) {
        if (j != classIndex) {
          incorrect+=m_ConfusionMatrix[i][j];
        }
        total+=m_ConfusionMatrix[i][j];
      }
    }
  }
  if (total == 0) {
    return 0;
  }
  return incorrect / total;
}
