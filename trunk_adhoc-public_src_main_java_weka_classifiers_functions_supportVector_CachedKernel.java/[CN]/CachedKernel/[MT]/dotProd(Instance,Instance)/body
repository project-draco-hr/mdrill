{
  double result=0;
  int n1=inst1.numValues();
  int n2=inst2.numValues();
  int classIndex=m_data.classIndex();
  for (int p1=0, p2=0; p1 < n1 && p2 < n2; ) {
    int ind1=inst1.index(p1);
    int ind2=inst2.index(p2);
    if (ind1 == ind2) {
      if (ind1 != classIndex) {
        result+=inst1.valueSparse(p1) * inst2.valueSparse(p2);
      }
      p1++;
      p2++;
    }
 else     if (ind1 > ind2) {
      p2++;
    }
 else {
      p1++;
    }
  }
  return (result);
}
