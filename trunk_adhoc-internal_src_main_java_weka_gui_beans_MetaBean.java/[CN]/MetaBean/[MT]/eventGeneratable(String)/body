{
  for (int i=0; i < m_subFlow.size(); i++) {
    BeanInstance output=(BeanInstance)m_subFlow.elementAt(i);
    if (output.getBean() instanceof EventConstraints) {
      if (((EventConstraints)output.getBean()).eventGeneratable(eventName)) {
        return true;
      }
    }
  }
  return false;
}
