{
  if (buffer.length() > 5 && buffer.substring(buffer.length() - 5,buffer.length()).equals("erin*")) {
    buffer.deleteCharAt(buffer.length() - 1);
    strip(buffer);
  }
  if (buffer.length() > 0 && buffer.charAt(buffer.length() - 1) == ('z')) {
    buffer.setCharAt(buffer.length() - 1,'x');
  }
}
