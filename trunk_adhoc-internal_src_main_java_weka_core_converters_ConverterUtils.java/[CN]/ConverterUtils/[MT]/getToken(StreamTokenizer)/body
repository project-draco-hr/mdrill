{
  tokenizer.nextToken();
  if (tokenizer.ttype == StreamTokenizer.TT_EOL) {
    return;
  }
  if ((tokenizer.ttype == '\'') || (tokenizer.ttype == '"')) {
    tokenizer.ttype=StreamTokenizer.TT_WORD;
  }
 else   if ((tokenizer.ttype == StreamTokenizer.TT_WORD) && (tokenizer.sval.equals("?"))) {
    tokenizer.ttype='?';
  }
}
