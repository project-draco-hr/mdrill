{
  String tmpStr;
  setDebug(Utils.getFlag('D',options));
  setSilent(Utils.getFlag('S',options));
  tmpStr=Utils.getOption('N',options);
  if (tmpStr.length() != 0)   setNumInstances(Integer.parseInt(tmpStr));
 else   setNumInstances(100);
  tmpStr=Utils.getOption('W',options);
  if (tmpStr.length() == 0)   throw new Exception("A estimator must be specified with the -W option.");
  setEstimator(Estimator.forName(tmpStr,Utils.partitionOptions(options)));
}
