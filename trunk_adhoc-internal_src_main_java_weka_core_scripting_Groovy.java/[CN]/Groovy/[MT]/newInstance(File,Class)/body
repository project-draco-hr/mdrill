{
  Object result;
  Object interpreter;
  Class cls;
  result=null;
  if (!isPresent())   return result;
  interpreter=newClassLoader();
  if (interpreter == null)   return result;
  try {
    cls=(Class)invoke(interpreter,"parseClass",new Class[]{File.class},new Object[]{file});
    result=cls.newInstance();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return result;
}
