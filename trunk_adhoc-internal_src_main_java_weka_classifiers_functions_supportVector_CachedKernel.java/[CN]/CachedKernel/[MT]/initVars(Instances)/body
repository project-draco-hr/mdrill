{
  super.initVars(data);
  m_kernelEvals=0;
  m_cacheHits=0;
  m_numInsts=m_data.numInstances();
  if (getCacheSize() > 0) {
    m_storage=new double[m_cacheSize * m_cacheSlots];
    m_keys=new long[m_cacheSize * m_cacheSlots];
  }
 else {
    m_storage=null;
    m_keys=null;
    m_kernelMatrix=null;
  }
}
