{
  double result=0;
  int n1=inst1.numValues();
  int n2=weights.length - 1;
  for (int p1=0, p2=0; p1 < n1 && p2 < n2; ) {
    int ind1=inst1.index(p1);
    int ind2=p2;
    if (ind1 == ind2) {
      if (ind1 != classIndex && !inst1.isMissingSparse(p1)) {
        result+=inst1.valueSparse(p1) * weights[p2];
      }
      p1++;
      p2++;
    }
 else     if (ind1 > ind2) {
      p2++;
    }
 else {
      p1++;
    }
  }
  return (result);
}
