{
  boolean[] result=new boolean[2];
  print("toString...");
  try {
    Classifier copy=(Classifier)m_Classifier.getClass().newInstance();
    copy.toString();
    result[0]=true;
    println("yes");
  }
 catch (  Exception e) {
    result[0]=false;
    println("no");
    if (m_Debug) {
      println("\n=== Full report ===");
      e.printStackTrace();
      println("\n");
    }
  }
  return result;
}
