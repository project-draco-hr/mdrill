{
  LogWindow log;
  LookAndFeel.setLookAndFeel();
  log=new LogWindow();
  log.setVisible(true);
  log.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
  System.out.print("a");
  System.err.print("a");
  System.out.print("a");
  System.out.println();
  System.err.println(new java.util.Date());
}
