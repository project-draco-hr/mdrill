{
  String[] superOpts=super.getOptions();
  String[] options=new String[superOpts.length + 1];
  int current=superOpts.length;
  for (int i=0; i < current; i++) {
    options[i]=superOpts[i];
  }
  if (getSaveInstances()) {
    options[current++]="-L";
  }
  while (current < options.length) {
    options[current++]="";
  }
  return options;
}
