{
  int size;
  int i;
  String cmd;
  size=Integer.parseInt(PROPERTIES.getProperty("HistorySize","50"));
  m_CommandHistory.clear();
  for (i=0; i < size; i++) {
    cmd=PROPERTIES.getProperty("Command" + i,"");
    if (cmd.length() != 0)     m_CommandHistory.add(cmd);
 else     break;
  }
  m_HistoryPos=m_CommandHistory.size();
}
