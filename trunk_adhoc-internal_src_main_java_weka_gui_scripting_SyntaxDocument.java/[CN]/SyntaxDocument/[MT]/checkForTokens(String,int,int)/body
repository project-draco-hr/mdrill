{
  while (startOffset <= endOffset) {
    while (isDelimiter(content.substring(startOffset,startOffset + 1))) {
      if (startOffset < endOffset)       startOffset++;
 else       return;
    }
    if (isQuoteDelimiter(content.substring(startOffset,startOffset + 1)))     startOffset=getQuoteToken(content,startOffset,endOffset);
 else     startOffset=getOtherToken(content,startOffset,endOffset);
  }
}
