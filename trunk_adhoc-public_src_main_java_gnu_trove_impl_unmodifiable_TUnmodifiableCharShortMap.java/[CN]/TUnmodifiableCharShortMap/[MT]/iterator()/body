{
  return new TCharShortIterator(){
    TCharShortIterator iter=m.iterator();
    public char key(){
      return iter.key();
    }
    public short value(){
      return iter.value();
    }
    public void advance(){
      iter.advance();
    }
    public boolean hasNext(){
      return iter.hasNext();
    }
    public short setValue(    short val){
      throw new UnsupportedOperationException();
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
