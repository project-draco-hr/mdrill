{
  m_KeyIndex=-1;
  try {
    if (m_ResultProducer != null) {
      String[] keyNames=m_ResultProducer.getKeyNames();
      for (int i=0; i < keyNames.length; i++) {
        if (keyNames[i].equals(m_KeyFieldName)) {
          m_KeyIndex=i;
          break;
        }
      }
    }
  }
 catch (  Exception ex) {
  }
  return m_KeyIndex;
}
