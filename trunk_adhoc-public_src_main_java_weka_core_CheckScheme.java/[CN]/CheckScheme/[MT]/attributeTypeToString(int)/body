{
  String result;
switch (type) {
case Attribute.NUMERIC:
    result="numeric";
  break;
case Attribute.NOMINAL:
result="nominal";
break;
case Attribute.STRING:
result="string";
break;
case Attribute.DATE:
result="date";
break;
case Attribute.RELATIONAL:
result="relational";
break;
default :
result="???";
}
return result;
}
