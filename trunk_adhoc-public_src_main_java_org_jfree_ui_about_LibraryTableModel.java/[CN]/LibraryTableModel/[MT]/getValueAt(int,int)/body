{
  Object result=null;
  final Library library=this.libraries[row];
  if (column == 0) {
    result=library.getName();
  }
 else   if (column == 1) {
    result=library.getVersion();
  }
 else   if (column == 2) {
    result=library.getLicenceName();
  }
 else   if (column == 3) {
    result=library.getInfo();
  }
  return result;
}
