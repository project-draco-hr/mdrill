{
  Vector result=new Vector();
  StringTokenizer tk=new StringTokenizer(rawString,m_Seperator);
  while (tk.hasMoreTokens())   result.addElement(tk.nextToken());
  String[] newStrings=new String[result.size()];
  for (int i=0; i < result.size(); i++)   newStrings[i]=(String)result.elementAt(i);
  return newStrings;
}
