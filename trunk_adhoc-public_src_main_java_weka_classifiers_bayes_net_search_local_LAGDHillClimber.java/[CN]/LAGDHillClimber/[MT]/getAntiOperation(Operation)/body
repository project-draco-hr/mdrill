{
  if (oOperation.m_nOperation == Operation.OPERATION_ADD)   return (new Operation(oOperation.m_nTail,oOperation.m_nHead,Operation.OPERATION_DEL));
 else {
    if (oOperation.m_nOperation == Operation.OPERATION_DEL)     return (new Operation(oOperation.m_nTail,oOperation.m_nHead,Operation.OPERATION_ADD));
 else {
      return (new Operation(oOperation.m_nHead,oOperation.m_nTail,Operation.OPERATION_REVERSE));
    }
  }
}
