{
  for (int i=0; i < m_nTabuList; i++) {
    Operation o=m_oTabuList[i];
    if (o != null) {
      if (o.m_nOperation == 0) {
        System.out.print(" +(");
      }
 else {
        System.out.print(" -(");
      }
      System.out.print(o.m_nTail + "->" + o.m_nHead+ ")");
    }
  }
  System.out.println();
}
