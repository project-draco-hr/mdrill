{
  String tmpStr;
  super.setOptions(options);
  tmpStr=Utils.getOption('W',options);
  if (tmpStr.length() == 0)   tmpStr=weka.classifiers.rules.ZeroR.class.getName();
  setObject(Utils.forName(Object.class,tmpStr,null));
  tmpStr=Utils.getOption("ignored",options);
  if (tmpStr.length() == 0)   tmpStr="capabilities,options";
  setIgnoredProperties(tmpStr);
}
