{
  int i;
  int size;
  String[][] data;
  String result;
  result="";
  data=new String[m_HeaderKeys.size()][2];
  for (i=0; i < m_HeaderKeys.size(); i++) {
    data[i][0]=m_HeaderKeys.get(i).toString() + ":";
    data[i][1]=m_HeaderValues.get(i).toString();
  }
  size=getColSize(data,0);
  for (i=0; i < data.length; i++)   data[i][0]=padString(data[i][0],size);
  for (i=0; i < data.length; i++)   result+=data[i][0] + " " + data[i][1]+ "\n";
  return result;
}
