{
  text.append(m_localModel.leftSide(m_train));
  text.append(m_localModel.rightSide(indeX,m_train));
  if (m_sons[indeX].m_isLeaf) {
    text.append(": ");
    text.append(m_localModel.dumpLabel(indeX,m_train) + "\n");
  }
 else {
    text.append(" AND\n");
    m_sons[indeX].dumpDecList(text);
  }
}
