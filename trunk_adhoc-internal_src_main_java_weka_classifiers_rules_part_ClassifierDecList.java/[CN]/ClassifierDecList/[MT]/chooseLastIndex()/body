{
  int minIndex=0;
  double estimated, min=Double.MAX_VALUE;
  if (!m_isLeaf)   for (int i=0; i < m_sons.length; i++)   if (son(i) != null) {
    if (Utils.grOrEq(localModel().distribution().perBag(i),(double)m_minNumObj)) {
      estimated=son(i).getSizeOfBranch();
      if (Utils.sm(estimated,min)) {
        min=estimated;
        minIndex=i;
      }
    }
  }
  return minIndex;
}
