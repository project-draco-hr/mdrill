{
  println("OptionHandler: " + m_OptionHandler.getClass().getName() + "\n");
  if (getDebug()) {
    println("--> Info");
    print("Default options: ");
    println(printOptions(getDefaultOptions()));
    print("User options: ");
    println(printOptions(getUserOptions()));
  }
  println("--> Tests");
  m_Success=checkListOptions();
  if (m_Success)   m_Success=checkSetOptions();
  if (m_Success)   m_Success=checkDefaultOptions();
  if (m_Success)   m_Success=checkRemainingOptions();
  if (m_Success)   m_Success=checkCanonicalUserOptions();
  if (m_Success)   m_Success=checkResettingOptions();
}
