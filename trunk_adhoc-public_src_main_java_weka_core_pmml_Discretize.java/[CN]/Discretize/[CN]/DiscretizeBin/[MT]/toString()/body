{
  StringBuffer buff=new StringBuffer();
  buff.append("\"" + m_binValue + "\" if value in: ");
  boolean first=true;
  for (  FieldMetaInfo.Interval i : m_intervals) {
    if (!first) {
      buff.append(", ");
    }
 else {
      first=false;
    }
    buff.append(i.toString());
  }
  return buff.toString();
}
