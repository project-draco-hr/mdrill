{
  w.write("% Rows\tColumns\n");
  w.write("" + getRowDimension() + "\t"+ getColumnDimension()+ "\n");
  w.write("% Matrix elements\n");
  for (int i=0; i < getRowDimension(); i++) {
    for (int j=0; j < getColumnDimension(); j++)     w.write("" + get(i,j) + "\t");
    w.write("\n");
  }
  w.flush();
}
