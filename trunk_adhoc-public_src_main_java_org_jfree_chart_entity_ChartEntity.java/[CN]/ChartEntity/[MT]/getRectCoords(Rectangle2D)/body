{
  if (rectangle == null) {
    throw new IllegalArgumentException("Null 'rectangle' argument.");
  }
  int x1=(int)rectangle.getX();
  int y1=(int)rectangle.getY();
  int x2=x1 + (int)rectangle.getWidth();
  int y2=y1 + (int)rectangle.getHeight();
  if (x2 == x1) {
    x2++;
  }
  if (y2 == y1) {
    y2++;
  }
  return x1 + "," + y1+ ","+ x2+ ","+ y2;
}
