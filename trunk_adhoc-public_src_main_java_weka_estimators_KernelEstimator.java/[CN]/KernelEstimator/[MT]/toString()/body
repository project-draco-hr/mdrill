{
  String result=m_NumValues + " Normal Kernels. \nStandardDev = " + Utils.doubleToString(m_StandardDev,6,4)+ " Precision = "+ m_Precision;
  if (m_NumValues == 0) {
    result+="  \nMean = 0";
  }
 else {
    result+="  \nMeans =";
    for (int i=0; i < m_NumValues; i++) {
      result+=" " + m_Values[i];
    }
    if (!m_AllWeightsOne) {
      result+="\nWeights = ";
      for (int i=0; i < m_NumValues; i++) {
        result+=" " + m_Weights[i];
      }
    }
  }
  return result + "\n";
}
