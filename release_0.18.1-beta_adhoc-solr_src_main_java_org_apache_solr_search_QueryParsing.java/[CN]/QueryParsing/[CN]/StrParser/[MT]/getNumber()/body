{
  eatws();
  int start=pos;
  boolean flt=false;
  while (pos < end) {
    char ch=val.charAt(pos);
    if ((ch >= '0' && ch <= '9') || ch == '+' || ch == '-') {
      pos++;
    }
 else     if (ch == '.' || ch == 'e' || ch == 'E') {
      flt=true;
      pos++;
    }
 else {
      break;
    }
  }
  String v=val.substring(start,pos);
  return flt ? Double.parseDouble(v) : Long.parseLong(v);
}
