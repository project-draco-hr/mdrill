{
  if (getInputFormat() == null) {
    throw new IllegalStateException("No input instance format defined");
  }
  if (m_CutPoints == null) {
    calculateCutPoints();
    setOutputFormat();
    for (int i=0; i < getInputFormat().numInstances(); i++) {
      convertInstance(getInputFormat().instance(i));
    }
  }
  flushInput();
  m_NewBatch=true;
  return (numPendingOutput() != 0);
}
