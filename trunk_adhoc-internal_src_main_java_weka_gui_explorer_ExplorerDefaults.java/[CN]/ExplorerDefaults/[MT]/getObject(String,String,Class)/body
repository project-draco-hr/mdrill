{
  Object result;
  String tmpStr;
  String[] tmpOptions;
  result=null;
  try {
    tmpStr=get(property,defaultValue);
    tmpOptions=Utils.splitOptions(tmpStr);
    if (tmpOptions.length != 0) {
      tmpStr=tmpOptions[0];
      tmpOptions[0]="";
      result=Utils.forName(cls,tmpStr,tmpOptions);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    result=null;
  }
  return result;
}
