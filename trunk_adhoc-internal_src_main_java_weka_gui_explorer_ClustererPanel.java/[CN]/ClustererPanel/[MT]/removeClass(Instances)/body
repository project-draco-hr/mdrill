{
  Remove af=new Remove();
  Instances retI=null;
  try {
    if (inst.classIndex() < 0) {
      retI=inst;
    }
 else {
      af.setAttributeIndices("" + (inst.classIndex() + 1));
      af.setInvertSelection(false);
      af.setInputFormat(inst);
      retI=Filter.useFilter(inst,af);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return retI;
}
