{
  logger.info("close " + this.filePath.getName() + " tlSum="+ tlSum+ ",tlCount="+ tlCount);
  if (!isClone) {
    if (isOpen) {
synchronized (descriptor) {
        descriptor.close();
      }
      isOpen=false;
    }
 else {
      throw new IOException("Index file " + filePath + " already closed");
    }
  }
}
