{
  boolean inside=true;
  for (int i=0; i < m_Instances.numAttributes(); i++) {
    if (x.value(i) < node.m_NodeRanges[i][MIN]) {
      x.setValue(i,node.m_NodeRanges[i][MIN]);
      inside=false;
    }
 else     if (x.value(i) > node.m_NodeRanges[i][MAX]) {
      x.setValue(i,node.m_NodeRanges[i][MAX]);
      inside=false;
    }
  }
  return inside;
}
