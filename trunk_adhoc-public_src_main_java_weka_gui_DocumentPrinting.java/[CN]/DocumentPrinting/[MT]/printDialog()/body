{
  if (m_PrinterJob.printDialog()) {
    m_PrinterJob.setPrintable(this,m_PageFormat);
    try {
      m_PrinterJob.print();
    }
 catch (    PrinterException printerException) {
      m_PageStartY=0;
      m_PageEndY=0;
      m_CurrentPage=-1;
      System.out.println("Error Printing Document");
    }
  }
}
