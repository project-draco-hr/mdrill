{
switch (this.position) {
case (START):
    return period.getFirstMillisecond(this.workingCalendar);
case (MIDDLE):
  return period.getMiddleMillisecond(this.workingCalendar);
case (END):
return period.getLastMillisecond(this.workingCalendar);
default :
return period.getMiddleMillisecond(this.workingCalendar);
}
}
