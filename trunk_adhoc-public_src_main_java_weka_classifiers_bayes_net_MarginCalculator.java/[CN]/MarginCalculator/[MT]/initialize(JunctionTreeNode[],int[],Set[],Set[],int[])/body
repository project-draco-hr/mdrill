{
  int nNodes=order.length;
  for (int i=nNodes - 1; i >= 0; i--) {
    int iNode=order[i];
    if (jtNodes[iNode] != null) {
      jtNodes[iNode].initializeUp();
    }
  }
  for (int i=0; i < nNodes; i++) {
    int iNode=order[i];
    if (jtNodes[iNode] != null) {
      jtNodes[iNode].initializeDown(false);
    }
  }
}
