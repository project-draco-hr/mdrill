{
  int i;
  if (!m_StreamableChecked) {
    m_Streamable=true;
    m_StreamableChecked=true;
    for (i=0; i < getFilters().length; i++) {
      if (getFilter(i) instanceof MultiFilter)       m_Streamable=((MultiFilter)getFilter(i)).isStreamableFilter();
 else       if (getFilter(i) instanceof StreamableFilter)       m_Streamable=true;
 else       m_Streamable=false;
      if (!m_Streamable)       break;
    }
    if (getDebug())     System.out.println("Streamable: " + m_Streamable);
  }
  return m_Streamable;
}
