{
  Integer rc=(Integer)nst.get("rc");
switch (rc) {
case 0:
{
      groupby=new RecordCount(key,nst);
      break;
    }
case 1:
{
    groupby=new GroupbyRow(key,nst);
    break;
  }
case 2:
{
  groupby=new SelectDetailRow(key,nst);
  break;
}
case 3:
{
groupby=new RecordCountDetail(key,nst);
break;
}
default :
{
groupby=new RecordCount(key,nst);
}
}
}
