{
  Iterator<Entry<K,V>> i=entrySet().iterator();
  if (value == null) {
    while (i.hasNext()) {
      Entry<K,V> e=i.next();
      if (e.getValue() == null)       return true;
    }
  }
 else {
    while (i.hasNext()) {
      Entry<K,V> e=i.next();
      if (value.equals(e.getValue()))       return true;
    }
  }
  return false;
}
