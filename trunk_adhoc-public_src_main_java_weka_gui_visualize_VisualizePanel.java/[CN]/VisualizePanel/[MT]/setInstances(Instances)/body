{
  if (inst.numAttributes() > 0 && inst.numInstances() > 0) {
    newColorAttribute(inst.numAttributes() - 1,inst);
  }
  PlotData2D temp=new PlotData2D(inst);
  temp.setPlotName(inst.relationName());
  try {
    setMasterPlot(temp);
  }
 catch (  Exception ex) {
    System.err.println(ex);
    ex.printStackTrace();
  }
}
