{
  if (m_splitListener == null) {
    m_submit.setText("Reset");
    m_submit.setActionCommand("Reset");
    if (m_originalPlot == null || m_originalPlot.m_plotInstances == m_plotInstances) {
      m_submit.setEnabled(false);
    }
 else {
      m_submit.setEnabled(true);
    }
  }
 else {
    m_submit.setEnabled(false);
  }
  m_createShape=false;
  m_shapePoints=null;
  m_shapes=null;
  this.repaint();
}
