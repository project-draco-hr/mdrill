{
  try {
    if (args.length == 0) {
      throw new Exception("The first argument must be the class name of a kernel");
    }
    String kernel=args[0];
    args[0]="";
    System.out.println(evaluate(kernel,args));
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    System.err.println(ex.getMessage());
  }
}
