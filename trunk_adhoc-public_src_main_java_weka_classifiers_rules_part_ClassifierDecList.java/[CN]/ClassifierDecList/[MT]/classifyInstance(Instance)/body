{
  double maxProb=-1;
  double currentProb;
  int maxIndex=0;
  int j;
  for (j=0; j < instance.numClasses(); j++) {
    currentProb=getProbs(j,instance,1);
    if (Utils.gr(currentProb,maxProb)) {
      maxIndex=j;
      maxProb=currentProb;
    }
  }
  if (Utils.eq(maxProb,0))   return -1.0;
 else   return (double)maxIndex;
}
