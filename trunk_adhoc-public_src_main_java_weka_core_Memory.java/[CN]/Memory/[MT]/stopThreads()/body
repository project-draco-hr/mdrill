{
  int i;
  Thread[] thGroup;
  Thread t;
  thGroup=new Thread[Thread.activeCount()];
  Thread.enumerate(thGroup);
  for (i=0; i < thGroup.length; i++) {
    t=thGroup[i];
    if (t != null) {
      if (t != Thread.currentThread()) {
        if (t.getName().startsWith("Thread"))         t.stop();
 else         if (t.getName().startsWith("AWT-EventQueue"))         t.stop();
      }
    }
  }
  thGroup=null;
  System.gc();
}
