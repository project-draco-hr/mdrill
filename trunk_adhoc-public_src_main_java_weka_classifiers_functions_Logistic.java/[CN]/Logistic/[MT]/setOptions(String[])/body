{
  setDebug(Utils.getFlag('D',options));
  String ridgeString=Utils.getOption('R',options);
  if (ridgeString.length() != 0)   m_Ridge=Double.parseDouble(ridgeString);
 else   m_Ridge=1.0e-8;
  String maxItsString=Utils.getOption('M',options);
  if (maxItsString.length() != 0)   m_MaxIts=Integer.parseInt(maxItsString);
 else   m_MaxIts=-1;
}
