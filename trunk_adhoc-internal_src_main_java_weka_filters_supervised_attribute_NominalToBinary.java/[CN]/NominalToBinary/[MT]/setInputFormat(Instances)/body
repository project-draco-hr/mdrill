{
  super.setInputFormat(instanceInfo);
  if (instanceInfo.classIndex() < 0) {
    throw new UnassignedClassException("No class has been assigned to the instances");
  }
  setOutputFormat();
  m_Indices=null;
  if (instanceInfo.classAttribute().isNominal()) {
    return true;
  }
 else {
    return false;
  }
}
