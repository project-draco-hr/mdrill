{
switch (opp) {
case 'l':
case 'b':
case 'c':
case 'e':
case 's':
case 'f':
case 'h':
case 'r':
case 't':
case 'n':
    return 3;
case '^':
  return 2;
case '*':
return 2;
case '/':
return 2;
case '+':
return 1;
case '-':
return 1;
case '(':
return 4;
case ')':
return 0;
default :
throw new IllegalArgumentException("Unrecognized operator:" + opp);
}
}
