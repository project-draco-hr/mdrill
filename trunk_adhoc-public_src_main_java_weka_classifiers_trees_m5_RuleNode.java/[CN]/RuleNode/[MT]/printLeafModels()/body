{
  StringBuffer text=new StringBuffer();
  if (m_isLeaf) {
    text.append("\nLM num: " + m_leafModelNum);
    text.append(m_nodeModel.toString());
    text.append("\n");
  }
 else {
    text.append(m_left.printLeafModels());
    text.append(m_right.printLeafModels());
  }
  return text.toString();
}
