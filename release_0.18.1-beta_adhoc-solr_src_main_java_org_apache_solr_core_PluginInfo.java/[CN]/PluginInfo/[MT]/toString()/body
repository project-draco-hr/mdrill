{
  StringBuilder sb=new StringBuilder("{");
  if (type != null)   sb.append("type = " + type + ",");
  if (name != null)   sb.append("name = " + name + ",");
  if (className != null)   sb.append("class = " + className + ",");
  if (initArgs.size() > 0)   sb.append("args = " + initArgs);
  sb.append("}");
  return sb.toString();
}
