{
  super.setUpComboBoxes(inst);
  m_XCombo.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
      setBorderText();
    }
  }
);
  m_YCombo.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
