{
  if (m_File != null)   ((AbstractFileLoader)m_Loader).setFile(m_File);
 else   if (m_URL != null)   ((URLSourcedLoader)m_Loader).setURL(m_URL.toString());
 else   if (m_Loader != null)   m_Loader.reset();
  m_BatchCounter=0;
  m_IncrementalBuffer=null;
  if (m_Loader != null) {
    if (!isIncremental())     m_BatchBuffer=m_Loader.getDataSet();
 else     m_BatchBuffer=null;
  }
}
