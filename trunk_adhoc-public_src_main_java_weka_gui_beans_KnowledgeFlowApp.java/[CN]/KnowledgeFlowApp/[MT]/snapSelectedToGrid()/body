{
  Vector v=m_mainKFPerspective.getSelectedBeans();
  if (v.size() > 0) {
    for (int i=0; i < v.size(); i++) {
      BeanInstance b=(BeanInstance)v.get(i);
      int x=b.getX();
      int y=b.getY();
      b.setXY(snapToGrid(x),snapToGrid(y));
    }
    revalidate();
    m_beanLayout.repaint();
    notifyIsDirty();
    m_mainKFPerspective.setEditedStatus(true);
  }
}
