{
  String result;
  int i;
  result="<table border=\"1\" cellpadding=\"3\" cellspacing=\"0\">\n" + "   <tr><td colspan=\"2\"><b>Key</b></td></tr>\n";
  for (i=0; i < getColCount(); i++) {
    if (getColHidden(i))     continue;
    result+="   <tr>" + "<td><b>(" + (i + 1) + ")</b></td>"+ "<td>"+ removeFilterName(m_ColNames[i])+ "</td>"+ "</tr>\n";
  }
  result+="</table>\n";
  return result;
}
