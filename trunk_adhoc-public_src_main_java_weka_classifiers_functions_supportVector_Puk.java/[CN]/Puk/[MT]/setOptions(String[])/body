{
  String tmpStr;
  tmpStr=Utils.getOption('O',options);
  if (tmpStr.length() != 0)   setOmega(Double.parseDouble(tmpStr));
 else   setOmega(1.0);
  tmpStr=Utils.getOption('S',options);
  if (tmpStr.length() != 0)   setSigma(Double.parseDouble(tmpStr));
 else   setSigma(1.0);
  super.setOptions(options);
}
