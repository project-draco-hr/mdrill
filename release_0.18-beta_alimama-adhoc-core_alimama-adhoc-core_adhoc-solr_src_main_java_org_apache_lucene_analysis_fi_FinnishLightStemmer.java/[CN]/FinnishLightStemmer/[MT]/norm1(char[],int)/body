{
  if (len > 5 && endsWith(s,len,"hde")) {
    s[len - 3]='k';
    s[len - 2]='s';
    s[len - 1]='i';
  }
  if (len > 4) {
    if (endsWith(s,len,"ei") || endsWith(s,len,"at"))     return len - 2;
  }
  if (len > 3) switch (s[len - 1]) {
case 't':
case 's':
case 'j':
case 'e':
case 'a':
case 'i':
    return len - 1;
}
return len;
}
