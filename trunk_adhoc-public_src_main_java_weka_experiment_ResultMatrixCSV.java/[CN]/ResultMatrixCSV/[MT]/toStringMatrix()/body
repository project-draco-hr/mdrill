{
  StringBuffer result;
  String[][] cells;
  int i;
  int n;
  result=new StringBuffer();
  cells=toArray();
  for (i=0; i < cells.length; i++) {
    for (n=0; n < cells[i].length; n++) {
      if (n > 0)       result.append(",");
      result.append(Utils.quote(cells[i][n]));
    }
    result.append("\n");
  }
  return result.toString();
}
