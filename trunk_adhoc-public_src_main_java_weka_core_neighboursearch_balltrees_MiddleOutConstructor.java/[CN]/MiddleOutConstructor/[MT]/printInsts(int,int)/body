{
  StringBuffer bf=new StringBuffer();
  try {
    bf.append("i: ");
    for (int i=startIdx; i <= endIdx; i++) {
      if (i == startIdx)       bf.append("" + m_InstList[i]);
 else       bf.append(", " + m_InstList[i]);
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
  return bf.toString();
}
