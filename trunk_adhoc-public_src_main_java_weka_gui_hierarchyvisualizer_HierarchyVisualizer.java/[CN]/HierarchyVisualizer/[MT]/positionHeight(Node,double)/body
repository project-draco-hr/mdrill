{
  if (node.isLeaf()) {
    node.m_fPosY=fOffSet + node.m_fLength;
    return node.m_fPosY;
  }
 else {
    double fPosY=fOffSet + node.m_fLength;
    double fYMax=0;
    for (int i=0; i < node.m_children.length; i++) {
      fYMax=Math.max(fYMax,positionHeight(node.m_children[i],fPosY));
    }
    node.m_fPosY=fPosY;
    return fYMax;
  }
}
