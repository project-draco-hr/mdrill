{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  LLRect other=(LLRect)obj;
  if (ll == null) {
    if (other.ll != null)     return false;
  }
 else   if (!ll.equals(other.ll))   return false;
  if (ur == null) {
    if (other.ur != null)     return false;
  }
 else   if (!ur.equals(other.ur))   return false;
  return true;
}
