{
  Vector<Integer> indices;
  int i;
  int n;
  boolean covered;
  indices=new Vector<Integer>();
  for (i=0; i < data.numAttributes(); i++) {
    if (i == data.classIndex())     continue;
    covered=false;
    for (n=0; n < getRanges().length; n++) {
      if (getRanges()[n].isInRange(i)) {
        covered=true;
        break;
      }
    }
    if (!covered)     indices.add(new Integer(i));
  }
  m_IndicesUnused=new int[indices.size()];
  for (i=0; i < indices.size(); i++)   m_IndicesUnused[i]=indices.get(i).intValue();
  if (getDebug())   System.out.println("Unused indices: " + Utils.arrayToString(m_IndicesUnused));
}
