{
  StringBuffer result;
  if (m_Model == null) {
    result=new StringBuffer("No model loaded yet.");
  }
 else {
    result=new StringBuffer();
    result.append("SerializedClassifier\n");
    result.append("====================\n\n");
    result.append("File: " + getModelFile() + "\n\n");
    result.append(m_Model.toString());
  }
  return result.toString();
}
