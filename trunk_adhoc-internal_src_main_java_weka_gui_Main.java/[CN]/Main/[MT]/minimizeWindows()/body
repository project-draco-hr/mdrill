{
  Iterator iter;
  Container frame;
  iter=getWindowList();
  while (iter.hasNext()) {
    frame=(Container)iter.next();
    try {
      if (frame instanceof ChildFrameMDI)       ((ChildFrameMDI)frame).setIcon(true);
 else       if (frame instanceof ChildFrameSDI)       ((ChildFrameSDI)frame).setExtendedState(JFrame.ICONIFIED);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
