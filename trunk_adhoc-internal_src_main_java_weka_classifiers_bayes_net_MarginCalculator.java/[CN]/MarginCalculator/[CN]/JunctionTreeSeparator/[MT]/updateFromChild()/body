{
  double[] fis=update(m_childNode);
  if (fis == null) {
    m_fiChild=null;
  }
 else {
    m_fiChild=fis;
    double sum=0;
    for (int iPos=0; iPos < m_nCardinality; iPos++) {
      sum+=m_fiChild[iPos];
    }
    for (int iPos=0; iPos < m_nCardinality; iPos++) {
      m_fiChild[iPos]/=sum;
    }
  }
}
