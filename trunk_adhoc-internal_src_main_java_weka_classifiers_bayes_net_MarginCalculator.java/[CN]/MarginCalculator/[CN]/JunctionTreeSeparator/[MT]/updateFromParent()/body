{
  double[] fis=update(m_parentNode);
  if (fis == null) {
    m_fiParent=null;
  }
 else {
    m_fiParent=fis;
    double sum=0;
    for (int iPos=0; iPos < m_nCardinality; iPos++) {
      sum+=m_fiParent[iPos];
    }
    for (int iPos=0; iPos < m_nCardinality; iPos++) {
      m_fiParent[iPos]/=sum;
    }
  }
}
