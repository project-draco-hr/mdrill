{
  m_MeanPrec=getDefaultMeanPrec();
  m_StdDevPrec=getDefaultStdDevPrec();
  m_ShowStdDev=getDefaultShowStdDev();
  m_ShowAverage=getDefaultShowAverage();
  m_RemoveFilterName=getDefaultRemoveFilterName();
