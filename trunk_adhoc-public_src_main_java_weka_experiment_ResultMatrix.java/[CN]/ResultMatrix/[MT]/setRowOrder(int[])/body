{
  int i;
  if (order == null) {
    m_RowOrder=null;
  }
 else {
    if (order.length == getRowCount()) {
      m_RowOrder=new int[order.length];
      for (i=0; i < order.length; i++)       m_RowOrder[i]=order[i];
    }
 else {
      System.err.println("setRowOrder: length does not match (" + order.length + " <> "+ getRowCount()+ ") - ignored!");
    }
  }
}
