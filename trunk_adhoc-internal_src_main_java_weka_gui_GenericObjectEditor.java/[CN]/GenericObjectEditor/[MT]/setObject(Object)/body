{
  boolean trueChange;
  if (getValue() != null) {
    trueChange=(!c.equals(getValue()));
  }
 else   trueChange=true;
  m_Backup=m_Object;
  m_Object=c;
  if (m_EditorComponent != null) {
    m_EditorComponent.updateChildPropertySheet();
  }
  if (trueChange) {
    m_Support.firePropertyChange("",null,null);
  }
}
