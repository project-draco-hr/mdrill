{
  String result;
  result=super.toString();
  if (m_CapabilitiesFilter != null) {
    initCapabilities();
    if (m_Capabilities != null) {
      if (m_Capabilities.supportsMaybe(m_CapabilitiesFilter) && !m_Capabilities.supports(m_CapabilitiesFilter))       result="<html><font color=\"" + MAYBE_SUPPORT + "\">"+ result+ "</font></i><html>";
 else       if (!m_Capabilities.supports(m_CapabilitiesFilter))       result="<html><font color=\"" + NO_SUPPORT + "\">"+ result+ "</font></i><html>";
    }
  }
  return result;
}
