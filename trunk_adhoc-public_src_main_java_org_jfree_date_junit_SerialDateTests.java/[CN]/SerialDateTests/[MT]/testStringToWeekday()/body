{
  int weekday=SerialDate.stringToWeekdayCode("Wednesday");
  assertEquals(SerialDate.WEDNESDAY,weekday);
  weekday=SerialDate.stringToWeekdayCode(" Wednesday ");
  assertEquals(SerialDate.WEDNESDAY,weekday);
  weekday=SerialDate.stringToWeekdayCode("Wed");
  assertEquals(SerialDate.WEDNESDAY,weekday);
}
