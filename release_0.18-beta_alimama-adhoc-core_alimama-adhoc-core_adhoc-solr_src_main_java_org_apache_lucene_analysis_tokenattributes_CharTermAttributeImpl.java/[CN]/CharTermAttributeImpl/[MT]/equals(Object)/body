{
  if (other == this) {
    return true;
  }
  if (other instanceof CharTermAttributeImpl) {
    final CharTermAttributeImpl o=((CharTermAttributeImpl)other);
    if (termLength != o.termLength)     return false;
    for (int i=0; i < termLength; i++) {
      if (termBuffer[i] != o.termBuffer[i]) {
        return false;
      }
    }
    return true;
  }
  return false;
}
