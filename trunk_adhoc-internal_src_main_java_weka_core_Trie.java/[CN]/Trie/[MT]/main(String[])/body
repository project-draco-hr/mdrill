{
  String[] data;
  if (args.length == 0) {
    data=new String[3];
    data[0]="this is a test";
    data[1]="this is another test";
    data[2]="and something else";
  }
 else {
    data=args.clone();
  }
  Trie t=new Trie();
  for (int i=0; i < data.length; i++)   t.add(data[i]);
  System.out.println(t);
}
