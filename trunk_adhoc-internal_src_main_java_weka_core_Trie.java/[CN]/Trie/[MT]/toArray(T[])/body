{
  T[] result;
  Iterator<T> iter;
  Vector<T> list;
  int i;
  list=new Vector<T>();
  iter=Utils.<Iterator<T>>cast(iterator());
  while (iter.hasNext())   list.add(iter.next());
  if (Array.getLength(a) != list.size())   result=Utils.<T[]>cast(Array.newInstance(a.getClass().getComponentType(),list.size()));
 else   result=a;
  for (i=0; i < list.size(); i++)   result[i]=list.get(i);
  return result;
}
