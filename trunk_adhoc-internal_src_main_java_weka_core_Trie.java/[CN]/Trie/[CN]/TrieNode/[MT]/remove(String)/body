{
  boolean result;
  Character c;
  String newSuffix;
  TrieNode child;
  c=suffix.charAt(0);
  newSuffix=suffix.substring(1);
  child=m_Children.get(c);
  if (child == null) {
    result=false;
  }
 else   if (newSuffix.length() == 0) {
    remove(c);
    result=true;
  }
 else {
    result=child.remove(newSuffix);
    if (child.getChildCount() == 0)     remove(child.getChar());
  }
  return result;
}
