{
  for (int j=0; j < m_instances.numAttributes(); j++) {
    if (!instance.isMissing(j)) {
      if (Double.isNaN(m_Min[j])) {
        m_Min[j]=instance.value(j);
        m_Max[j]=instance.value(j);
      }
 else       if (instance.value(j) < m_Min[j]) {
        m_Min[j]=instance.value(j);
      }
 else       if (instance.value(j) > m_Max[j]) {
        m_Max[j]=instance.value(j);
      }
    }
  }
}
