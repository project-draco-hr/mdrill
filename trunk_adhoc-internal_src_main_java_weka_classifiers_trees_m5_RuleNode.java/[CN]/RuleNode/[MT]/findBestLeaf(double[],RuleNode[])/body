{
  if (!m_isLeaf) {
    if (m_left != null) {
      m_left.findBestLeaf(maxCoverage,bestLeaf);
    }
    if (m_right != null) {
      m_right.findBestLeaf(maxCoverage,bestLeaf);
    }
  }
 else {
    if (m_numInstances > maxCoverage[0]) {
      maxCoverage[0]=m_numInstances;
      bestLeaf[0]=this;
    }
  }
}
