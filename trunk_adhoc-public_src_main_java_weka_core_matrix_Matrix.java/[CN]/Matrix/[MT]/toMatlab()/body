{
  StringBuffer result;
  int i;
  int n;
  result=new StringBuffer();
  result.append("[");
  for (i=0; i < getRowDimension(); i++) {
    if (i > 0)     result.append("; ");
    for (n=0; n < getColumnDimension(); n++) {
      if (n > 0)       result.append(" ");
      result.append(Double.toString(get(i,n)));
    }
  }
  result.append("]");
  return result.toString();
}
