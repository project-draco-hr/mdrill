{
  Matrix X=new Matrix(i1 - i0 + 1,j1 - j0 + 1);
  double[][] B=X.getArray();
  try {
    for (int i=i0; i <= i1; i++) {
      for (int j=j0; j <= j1; j++) {
        B[i - i0][j - j0]=A[i][j];
      }
    }
  }
 catch (  ArrayIndexOutOfBoundsException e) {
    throw new ArrayIndexOutOfBoundsException("Submatrix indices");
  }
  return X;
}
